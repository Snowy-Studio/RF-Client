<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>net6.0-windows7.0</TargetFramework>
	  <LangVersion>13.0</LangVersion>
    <OutputType>Exe</OutputType>
    <Title>ClientUpdater</Title>
    <Description>Ra2Client Updater</Description>
    <Company>Snowy Studio</Company>
    <Product>ClientUpdater</Product>
    <Copyright>Copyright © Snowy Studio 2022-2025</Copyright>
    <Trademark>Snowy</Trademark>
    <AssemblyName>ClientUpdater</AssemblyName>
    <RootNamespace>ClientUpdater</RootNamespace>
    <Nullable>disable</Nullable>
    <ComVisible>false</ComVisible>
    <CLSCompliant>false</CLSCompliant>
    <AnalysisLevel>latest-all</AnalysisLevel>
    <EnforceCodeStyleInBuild>true</EnforceCodeStyleInBuild>
    <Platforms>AnyCPU</Platforms>
    <ComVisible>false</ComVisible>
    <CLSCompliant>false</CLSCompliant>
    <UseAppHost>false</UseAppHost>
    <AppendTargetFrameworkToOutputPath>false</AppendTargetFrameworkToOutputPath>
    <IsPublishable>False</IsPublishable>
    <AssemblyVersion>1.5.0.0</AssemblyVersion>
    <FileVersion>1.5.0.0</FileVersion>
    <ApplicationManifest>app.manifest</ApplicationManifest>
  </PropertyGroup>
  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
    <NoWarn>1701;1702;SA1633;</NoWarn>
	<OutputPath>..\Bin\Resources\Binaries</OutputPath>
  </PropertyGroup>
  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release|AnyCPU'">
    <NoWarn>1701;1702;SA1633;</NoWarn>
	<OutputPath>..\Bin\Resources\Binaries</OutputPath>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="StyleCop.Analyzers" Version="1.2.0-beta.435">
      <PrivateAssets>all</PrivateAssets>
      <IncludeAssets>runtime; build; native; contentfiles; analyzers; buildtransitive</IncludeAssets>
    </PackageReference>
  </ItemGroup>
</Project>